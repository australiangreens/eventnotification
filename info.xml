<?xml version="1.0"?>
<extension key="eventnotification" type="module">
  <file>eventnotification</file>
  <name>Australian Greens Event Notification Extension</name>
  <description>Notifies a specific email address when an event is updated or added if the domain has enabled sending and or a financial type is to be notified</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamuslee001@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/eventnotification</url>
    <url desc="Documentation">https://github.com/australiangreens/eventnotification</url>
    <url desc="Support">https://github.com/australiangreens/eventnotification</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-19</releaseDate>
  <version>1.2.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Eventnotification</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
